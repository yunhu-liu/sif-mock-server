  <con:testStep type="groovy" name="UpdatePayload" id="d4f34c34-0852-4e87-8419-6ffec478a740">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

// Load Data
def orgRefId = context.expand( '${GetOrgRefId#OrgRefId}' )
def scheduleLocalId = context.expand( '${Properties 1#ScheduleLocalId}' )
def dynamicId = context.expand( '${GenDynamicId#DynamicId}' ) 
def payload = context.expand( '${GetPayload#Payload}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count
def spCount = slurped.SchedulePeriods.SchedulePeriod.size()

// Loop Through and Update Fields for Each
for ( i = 0; i &lt; spCount; i++)
{
	// Get Values 
	def schedulePeriod = new JsonBuilder( slurped.SchedulePeriods.SchedulePeriod[i]) 
	def spLocalId = schedulePeriod.content.SchedulePeriodLocalId 
	def periodName = schedulePeriod.content.PeriodName
	
	// Update Fields
	slurped.SchedulePeriods.SchedulePeriod[i].SchedulePeriodLocalId = "$spLocalId" + "_" + "$dynamicId"
	slurped.SchedulePeriods.SchedulePeriod[i].PeriodName = "$periodName" + "_" + "$dynamicId"
	slurped.SchedulePeriods.SchedulePeriod[i].Organisation.RefId = "$orgRefId"
	slurped.SchedulePeriods.SchedulePeriod[i].Schedule.LocalId = "$scheduleLocalId"
}

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>
    </con:config>
  </con:testStep>

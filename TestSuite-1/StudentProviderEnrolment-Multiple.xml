  <con:testStep type="groovy" name="UpdatePayload" id="7f0f67a6-eb82-49d3-874e-f520c9d82698">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

def orgRefId = context.expand( '${#Project#OrgRefId2}' )

// Get Payload from Previous Test Case
def response = context.expand( '${#[TestSuite 1#StudentPersonal-Multiple#GetStudentPersonals]#Response}' )
def studentSlurped = new JsonSlurper().parseText(response)

// Load Data
def payload = context.expand( '${GetPayload#Payload}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count
def speCount = slurped.StudentProviderEnrolments.StudentProviderEnrolment.size()
def spCount = studentSlurped.StudentPersonals.StudentPersonal.size()
def lBound = speCount &lt; spCount ? speCount : spCount

// Loop Through and Update Fields for Each
for ( i = 0; i &lt; lBound; i++)
{
	def studentLocalId = slurped.StudentProviderEnrolments.StudentProviderEnrolment[i].Student.LocalId
	def nsn = studentSlurped.StudentPersonals.StudentPersonal[i].NationalStudentNumber
	
	// Update Fields
	slurped.StudentProviderEnrolments.StudentProviderEnrolment[i].StudentProviderEnrolmentLocalId = "$orgRefId" + "_" + "$studentLocalId" 
	slurped.StudentProviderEnrolments.StudentProviderEnrolment[i].Student &lt;&lt; [NationalStudentNumber : "$nsn"]	
	slurped.StudentProviderEnrolments.StudentProviderEnrolment[i].Provider.RefId = "$orgRefId"
	slurped.StudentProviderEnrolments.StudentProviderEnrolment[i].Student.remove("LocalId")	
}

while ( slurped.StudentProviderEnrolments.StudentProviderEnrolment.size() > lBound)
{
	slurped.StudentProviderEnrolments.StudentProviderEnrolment.remove(lBound)		
}

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>
    </con:config>
  </con:testStep>

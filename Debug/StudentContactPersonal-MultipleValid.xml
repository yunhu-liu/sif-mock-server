  <con:testStep type="groovy" name="UpdatePayload" id="d3ad5284-44ea-44e8-8742-8ff5d0223f94">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

// Load Data for Sa 
def dynamicId = context.expand( '${GenDynamicId#DynamicId}' ) 
def payload = context.expand( '${GetPayload#Payload}' )
def orgRefId = context.expand( '${GetOrgRefId#OrgRefId}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count of SCP
def scpCount = slurped.StudentContactPersonals.StudentContactPersonal.size()

// Loop and Update Fields
for ( i = 0; i &lt; scpCount; i++)
{
	// Get Values 
	def scp = new JsonBuilder(slurped.StudentContactPersonals.StudentContactPersonal[i]) 
	def scpLocalId = scp.content.StudentContactPersonalLocalId 
	def scpFName = scp.content.PersonInfo.Name.FamilyName
	def scpLName = scp.content.PersonInfo.Name.GivenName
	def scpName = scp.content.PersonInfo.Name.FullName
	def scpAddress = scp.content.PersonInfo.Email.Personal.Address
	
	// Update Fields
	slurped.StudentContactPersonals.StudentContactPersonal[i].StudentContactPersonalLocalId = "$scpLocalId" + "_" + "$dynamicId"
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Name.FamilyName = "$scpFName" + "_" + "$dynamicId"
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Name.GivenName = "$scpLName" + "_" + "$dynamicId"
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Name.FullName = "$scpName" + "_" + "$dynamicId"
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Email.Personal.SourceOrganisation.RefId = "$orgRefId"
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Email.Personal.Address = "$dynamicId" + "_" + "$scpAddress"
}

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>
    </con:config>

  <con:testStep type="groovy" name="UpdatePayload" id="74bd9444-eb56-4c5f-ba2d-17936ddacfbd">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

// Load Data
def orgRefId = context.expand( '${#Project#OrgRefId}' )
def payload = context.expand( '${GetPayload#Payload}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count
def scrCount = slurped.StudentContactRelationships.StudentContactRelationship.size()

// Loop Through and Update Fields for Each
for ( i = 0; i &lt; scrCount; i++)
{
	// Update Fields
	slurped.StudentContactRelationships.StudentContactRelationship[i].Organisation.RefId = "$orgRefId"
}

// Set Values to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "Count", scrCount.toString() )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentContactRelationshipLocalId", slurped.StudentContactRelationships.StudentContactRelationship[0].StudentContactRelationshipLocalId )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentLocalId", slurped.StudentContactRelationships.StudentContactRelationship[0].Student.LocalId )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentContactLocalId", slurped.StudentContactRelationships.StudentContactRelationship[0].StudentContact.LocalId )

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>

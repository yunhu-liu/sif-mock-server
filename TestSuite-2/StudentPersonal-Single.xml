  <con:testStep type="groovy" name="GetStudentRefId" id="f692dd3f-1831-44f7-b7d0-00a382c2d587">
    <con:settings/>
    <con:config>
      <script>// Get RefId from Previous Test Case

import groovy.json.*

// Get Payload from Previous Test Case
def response = context.expand( '${#[TestSuite 1#StudentPersonal-Multiple#GetStudentPersonals]#Response}' )
def slurped = new JsonSlurper().parseText(response)

// Get RefId from Random Record
def spCount = slurped.StudentPersonals.StudentPersonal.size()
Random random = new Random()
int rndIndex = random.nextInt(spCount)
def studentPersonalRefId = slurped.StudentPersonals.StudentPersonal[rndIndex].StudentPersonalRefId

// Set RefId to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentPersonalRefId", studentPersonalRefId )</script>

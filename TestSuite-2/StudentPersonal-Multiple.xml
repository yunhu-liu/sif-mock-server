  <con:testStep type="groovy" name="UpdatePayload" id="43c4c038-a38e-4318-a493-27d3837f85ec">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

// Load Data
def orgRefId = context.expand( '${#Project#OrgRefId}' )
def payload = context.expand( '${GetPayload#Payload}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count
def spCount = slurped.StudentPersonals.StudentPersonal.size()

// Loop Through and Update Fields for Each
for ( i = 0; i &lt; spCount; i++)
{
	def studentPeronsalLocalId = slurped.StudentPersonals.StudentPersonal[i].StudentPersonalLocalId

	// Update Fields
	slurped.StudentPersonals.StudentPersonal[i].PersonInfo.Email.Personal.SourceOrganisation.RefId = "$orgRefId"
	slurped.StudentPersonals.StudentPersonal[i] &lt;&lt; 
	[StudentProviderEnrolment: [
		StudentProviderEnrolmentLocalId: "2015_" + "$orgRefId" + "_" + "$studentPeronsalLocalId",
          EntryDate: "2015-01-29T00:00:00",
          SchoolEnrolment: [
          	YearLevel: 6,
          	FundingYearLevel: 7]  
          ]
	]
}

// Set Values to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentPersonalCount", spCount.toString() )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentPersonalLocalId", slurped.StudentPersonals.StudentPersonal[0].StudentPersonalLocalId )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "NationalStudentNumber", slurped.StudentPersonals.StudentPersonal[0].NationalStudentNumber )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "FamilyName", slurped.StudentPersonals.StudentPersonal[0].PersonInfo.Name.FamilyName )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "GivenName", slurped.StudentPersonals.StudentPersonal[0].PersonInfo.Name.GivenName )

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>
    </con:config>
  </con:testStep>

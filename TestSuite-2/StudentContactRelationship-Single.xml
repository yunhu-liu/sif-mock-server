    <con:settings/>
    <con:config>
      <script>// Get RefId from Previous Test Case

import groovy.json.*

// Get Payload from Previous Test Case
def response = context.expand( '${#[TestSuite 1#StudentContactRelationship-Multiple#GetStudentContacRelationships]#Response}' )
def slurped = new JsonSlurper().parseText(response)

// Get RefId from Random Record
def scpCount = slurped.StudentContactRelationships.StudentContactRelationship.size()
Random random = new Random()
int rndIndex = random.nextInt(scpCount)
def refId = slurped.StudentContactRelationships.StudentContactRelationship[rndIndex].StudentContactRelationshipRefId

// Set RefId to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentContactRelationshipRefId", refId )</script>

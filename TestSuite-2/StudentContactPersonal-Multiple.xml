  <con:testStep type="groovy" name="UpdatePayload" id="ecf5f342-3d58-4ff2-a1da-2a72e849bbb6">
    <con:settings/>
    <con:config>
      <script>// Update Payload by Adding Dynamic Values to Some Fields
import groovy.json.*

// Load Data
def orgRefId = context.expand( '${#Project#OrgRefId}' )
def payload = context.expand( '${GetPayload#Payload}' ) 
def slurped = new JsonSlurper().parseText(payload)

// Get Total Count
def scpCount = slurped.StudentContactPersonals.StudentContactPersonal.size()

// Loop Through and Update Fields for Each
for ( i = 0; i &lt; scpCount; i++)
{
	// Update Fields
	slurped.StudentContactPersonals.StudentContactPersonal[i].PersonInfo.Email.Personal.SourceOrganisation.RefId = "$orgRefId"
}

// Set Values to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentContactPersonalCount", scpCount.toString() )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StudentContactPersonalLocalId", slurped.StudentContactPersonals.StudentContactPersonal[0].StudentContactPersonalLocalId )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "FamilyName", slurped.StudentContactPersonals.StudentContactPersonal[0].PersonInfo.Name.FamilyName )
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "GivenName", slurped.StudentContactPersonals.StudentContactPersonal[0].PersonInfo.Name.GivenName )

// Write Updated Payload to Result
return new JsonBuilder(slurped)</script>
    </con:config>
  </con:testStep>

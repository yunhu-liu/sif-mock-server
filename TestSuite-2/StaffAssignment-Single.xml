  <con:testStep type="groovy" name="GetRefId" id="dcb1b2f0-bec0-4fab-8ac4-1b8135485f14">
    <con:settings/>
    <con:config>
      <script>// Get RefId from Previous Test Case

import groovy.json.*

// Get Payload from Previous Test Case
def response = context.expand( '${#[TestSuite 1#StaffAssignment-Multiple#GetStaffAssignments]#Response}' )
def slurped = new JsonSlurper().parseText(response)

// Get RefId from Random Record
def saCount = slurped.StaffAssignments.StaffAssignment.size()
Random random = new Random()
int rndIndex = random.nextInt(saCount)
def refId = slurped.StaffAssignments.StaffAssignment[rndIndex].StaffAssignmentRefId

// Set RefId to Property
testRunner.testCase.testSteps["Properties 1"].setPropertyValue( "StaffAssignmentRefId", refId )</script>
    </con:config>
  </con:testStep>
